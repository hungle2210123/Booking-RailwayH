# Example Environment Variables for Hotel Booking System

# Flask Configuration
FLASK_SECRET_KEY=your_secret_key_here

# Railway PostgreSQL Database (Production)
DATABASE_URL=postgresql://postgres:password@host:port/database

# Local PostgreSQL Database (for synchronization)
LOCAL_DATABASE_URL=postgresql://postgres:locloc123@localhost:5432/hotel_booking

# AI Configuration
GOOGLE_API_KEY=your_gemini_api_key_here

# Other Configuration
DEBUG=False
ENV=production