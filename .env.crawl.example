# Example environment variables for crawling functionality
# Copy to .env and fill in your actual API keys

# Firecrawl API Configuration
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# Browser Profile Configuration (optional)
CHROME_BINARY_PATH=/usr/bin/google-chrome
CHROME_DRIVER_PATH=/usr/bin/chromedriver

# Crawling Settings
MAX_CONCURRENT_CRAWLS=3
CRAWL_TIMEOUT_SECONDS=30
CRAWL_RETRY_ATTEMPTS=2

# Integration with existing hotel system
USE_EXISTING_AI_PROCESSING=true
SAVE_CRAWL_SCREENSHOTS=true